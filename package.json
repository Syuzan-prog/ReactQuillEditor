    "stylelint:fix": "cross-env NODE_ENV=production stylelint '**/*{.css,.scss}' --fix",
    "deploy": "npx s3-deploy './dist/**' --cwd './dist/' --region $AWS_REGION --bucket $S3_BUCKET_NAME --distId $CF_DIST_ID --invalidate '/*'",
